{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "main": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/beta",
            "index": "src/projects/main/index.html",
            "browser": "src/projects/main/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
             "src/assets"
            ],
            "styles": [
              "src/projects/main/styles.scss",
              "src/assets/swiper/swiper.css",
              "src/assets/dropzone/dropzone.css",
              "src/assets/wave/wave.css",
              "src/assets/bootstrap/css/pickr-themes.css",
               "src/assets/bootstrap/css/core.css",
               "src/assets/bootstrap/css/demo.css",
               "src/assets/bootstrap/css/front.css",
               "src/assets/bootstrap/css/front-landing.css",
               "node_modules/aos/dist/aos.css"
            ],
            "scripts": [
              "src/assets/bootstrap/js/bootstrap.min.js",
              "src/assets/dropzone/dropzone.js",
              "src/assets/swiper/swiper.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
             "outputHashing": "all",
             "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true
             }
            },
            "development": {
              "optimization": true,
              "extractLicenses": false,
              "sourceMap": true,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true
             }
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "main:build:production"
            },
            "development": {
              "buildTarget": "main:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/projects/main/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "login": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/beta/browser/login",
            "index": "src/projects/login/login.html",
            "browser": "src/projects/login/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
             "src/assets"
            ],
            "styles": [
              "src/projects/login/styles.scss",
              "src/assets/dropzone/dropzone.css",
              "src/assets/wave/wave.css",
              "src/assets/bootstrap/css/pickr-themes.css",
               "src/assets/bootstrap/css/core.css",
               "src/assets/datatable/datatables.bootstrap5.css",
               "src/assets/datatable/responsive.bootstrap5.css",
               "node_modules/aos/dist/aos.css"
            ],
            "scripts": [
              "src/assets/datatable/jquery.min.js",
              "src/assets/bootstrap/js/bootstrap.min.js",
              "src/assets/dropzone/dropzone.js",
              "src/assets/datatable/datatables-bootstrap5.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
             "outputHashing": "all",
             "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true
             }
            },
            "development": {
              "optimization": true,
              "extractLicenses": false,
              "sourceMap": true,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true
             }
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "login:build:production"
            },
            "development": {
              "buildTarget": "login:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/projects/login/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
